/*
reset:
*/ 
html
  font-size 10px

h1, h2, h3, h4, h5, h6
  margin 0
ul, ol
  padding 0
  margin 0
dl
  margin 0
  dt, dd
    margin 0
a
  color #14ade5

  &:hover
    color $hover_1
p
  padding 0
  margin 0

input, textarea, button, select
  outline none
i
  font-style normal

.hidden
  display none

body
  font-size 100%

h1
  fontsize 26px
  font-weight normal
  color #b7b8b8